{
  "name": "lunch-bot",
  "version": "1.0.0",
  "description": "A bot that alerts people of their lunch duties on Friday morning",
  "main": "app.js",
  "scripts": {
    "build": "babel src --out-dir dist",
    "getUsers": "node dist/getUsersFromSheets.js",
    "lunchmatch": "node dist/match.js",
    "buildandrun": "npm run build && npm run",
    "dryrun": "node dist/match.js --dry-run"
  },
  "babel": {
    "presets": [
      "latest-node"
    ]
  },
  "engines": {
    "node": "12.14.1"
  },
  "author": "Sparkbox",
  "license": "ISC",
  "dependencies": {
    "@slack/web-api": "^5.7.0",
    "body-parser": "^1.18.2",
    "csv-stringify": "^5.3.6",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "google-sheets-api": "^0.4.2",
    "googleapis": "^47.0.0",
    "lodash": "^4.17.15",
    "pg": "^7.4.1",
    "slack-client": "^2.0.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "babel-preset-latest-node": "^4.0.1",
    "eslint": "^3.7.1",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-plugin-flowtype": "^2.20.0",
    "eslint-plugin-import": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.4.1",
    "fuse.js": "^3.4.6"
  }
}
